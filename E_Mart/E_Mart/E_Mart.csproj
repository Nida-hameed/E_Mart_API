    </Compile>
    <Compile Update="Views\User\UserSideBarFlyout.xaml.cs">
      <DependentUpon>UserSideBarFlyout.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\User\UserSideBar.xaml.cs">
